{
  "name": "buzzni",
  "version": "1.0.0",
  "description": "buzzni project without frameworks. plain js",
  "main": "index.js",
  "scripts": {
    "pug": "pug -P ./src/index.pug --out ./dist",
    "sass": "node-sass ./src/sass/ -o ./dist/css/ --sourcemap",
    "dev": "rm -rf ./dist/* && npm run sass && npm run pug && cp -R ./src/js ./dist/js && ws"
  },
  "author": "jungmin ji",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "local-web-server": "^2.2.4",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6"
  }
}
